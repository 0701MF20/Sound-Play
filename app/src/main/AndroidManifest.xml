<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.soundplay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SoundPlay">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SeekbarActivity"
            android:label="Seek bar Basic"/>
        <activity android:name=".SoundoneActivity"
            android:label="Sound 1"/>
        <activity android:name=".SoundtwoActivity"
            android:label="Sound 2"/>
<activity android:name=".MusicplayerActivity"
    android:label="Music Player"/>
    </application>

</manifest>